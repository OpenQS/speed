{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Submission",
  "type": "object",
  "properties": {
    "Problem name": { "type": "string" },
    "N":            { "type": "integer" },
    "time":         { "type": "number" },
    "architecture": { "type": "string" },
    "doi": {
      "type": "string",
      "pattern": "^10\\.\\d{4,9}/[-._;()/:A-Za-z0-9]+$"
    },
    "extra":        { "type": "string" }
  },
  "required": ["Problem name", "N", "time", "architecture", "doi"],
  "additionalProperties": false
}